   agent any

   stages {
      stage('Build') {
         steps {
            echo 'building form jenkin-pipeline..'
         }
      }
      stage('Test') {
         steps {
            echo 'Testing form jenkin-pipeline..'
         }
      }
      stage('Deploy') {
         steps {
            echo 'Deploying form jenkin-pipeline..'
         }
      }
   }
}
